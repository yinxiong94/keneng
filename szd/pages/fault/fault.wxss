.db {
  width: 100%;
  background: rgba(228, 228, 228, 1);
  overflow: hidden;
}

.db>image {
  width: 100%;
  height: 60rpx;
}

.db .top {
  width: 694rpx;
  height: 122rpx;
  border-radius: 10rpx;
  background-color: #fff;
  margin: 0 auto;
  font-size: 31rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: 400;
  color: rgba(42, 42, 42, 1);
  text-align: center;
  line-height: 122rpx;
  position: absolute;
  top: 4rpx;
  left: 50%;
  transform: translateX(-50%);
}

.db .from {
  width: 694rpx;
  height: 886rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 150rpx;
  margin-bottom: 20rpx;
}

.db .from .list {
  height: 105rpx;
  width: 640rpx;
  margin: 0 auto;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
}

.db .from .list .left {
  font-size: 29rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(121, 121, 121, 1);
  text-align: center;
  width: 100rpx;
  height: 105rpx;
  line-height: 105rpx;
}

.db .from .list>input {
  flex: 1;
  height: 27rpx;
  font-size: 29rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(42, 42, 42, 1);
  margin-left: 50rpx;
  margin-top: 25rpx;
}
.db .from .list>image{
  width: 9rpx;
  height: 17rpx;
  margin-top: 45rpx;
}
.tj{
  width: 522rpx;
  height: 90rpx;
  border-radius: 45rpx;
  background: #8ec31f;
  margin: auto;
  margin-top: 288rpx;
  color:#fff;
  font-size: 36rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  text-align: center;
  line-height: 90rpx
}