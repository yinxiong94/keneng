<view class="db">
  <view class="top"></view>
  <view class='tx'>
    <image class="userinfo-avatar" src="{{list.UserImg}}" mode="cover"></image>
    <view class='xx'>
      <view class='t1'>{{list.ColonelName}}</view>
      <view class='t2'>
        <image src="/pages/img/phone.png" class='img'></image>{{tel}}</view>
    </view>
    <image src="/pages/img/vip.png" class='vip'></image>
  </view>
  <view class='list'>
    <image src="/pages/img/grzxbg.png" class='bg'></image>
    <view class='t_1'>我的钱包</view>
    <view class='t_2' bindtap='tofy'>{{list.BalancePrice}}</view>
    <view class='t_3'>资产总金额（元）</view>
    <view class='t_4' bindtap='totx'>申请提现</view>
  </view>

  <view class="item m330" bindtap='tozz'>
    <view class="left">
      <image lazy-load='true' src="/pages/img/zzsq.png" class='img0'></image>
      <text>见习社长转正申请</text>
    </view>
    <image class='right' src="/pages/img/right.png"></image>
  </view>

  <view class="item" bindtap='tomyhy'>
    <view class="left">
      <image lazy-load='true' src="/pages/img/myvip.png" class='img1'></image>
      <text>我的会员</text>
    </view>
    <image class='right' src="/pages/img/right.png"></image>
  </view>

  <view class="item" bindtap='totg'>
    <view class="left">
      <image lazy-load='true' src="/pages/img/mytg.png" class='img2'></image>
      <text>我的推广</text>
    </view>
    <image class='right' src="/pages/img/right.png"></image>
  </view>
  <view class="tabar">
    <view class="nav" bindtap='handJump1'>
      <image src='../img/wdsb1.png' lazy-load='true'></image>
      <text>我的设备</text>
    </view>
    <view class="nav" bindtap='handJump3'>
      <image src='../img/grzx.png' lazy-load='true'></image>
      <text class="lv">个人中心</text>
    </view>
  </view>
</view>