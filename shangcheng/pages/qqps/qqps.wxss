page {
  background-color: #efeff4;
}

.db .from {
  background-color: #fff;
  width: 690rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  border-radius: 10rpx;
  padding: 30rpx 30rpx;
  box-sizing: border-box;
}

.db .from .s {
  display: flex;
  justify-content: space-between;
}

.db .from .s .left {
  font-size: 32rpx;
  font-family: PingFangSC-;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.db .from .s .right {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(255, 164, 46, 1);
  margin-top: 6rpx;
}

.db .from .s1 {
  display: flex;
  margin-top: 30rpx;
}

.db .from .s1 image {
  width: 28rpx;
  height: 36rpx;
}

.db .from .s1 .right {
  flex: 1;
  margin-left: 24rpx;
  line-height: 36rpx;
  height: 36rpx;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(29, 32, 35, 1);
}

.db .from .remaining {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(29, 32, 35, 1);
  margin-top: 37rpx;
}

.db .from .remaining text {
  font-size: 48rpx;
  color: #ffa42e;
}

.btn {
  height: 60rpx;
  margin-top: 30rpx;
  display: flex;
  justify-content: flex-end;
}

.btn .pin {
  width: 180rpx;
  height: 60rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(213, 213, 213, 1);
  border-radius: 30rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(96, 105, 114, 1);
}

.btn .shou {
  width: 180rpx;
  height: 60rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(142, 195, 31, 1);
  border-radius: 30rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(142, 195, 31, 1);
  margin-left: 20rpx;
}

.goout {
  display: flex;
}

.goout_right {
  flex: 1;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(29, 32, 35, 1);
  margin-top: 37rpx;
  line-height: 78rpx;
  height: 60rpx;
  text-align: right;
}

.null {
  width: 750rpx;
  background-color: #efeff4;
  overflow: hidden;
}

.null .top {
  width: 710rpx;
  height: 500rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 10rpx;
  text-align: center;
}

.null .top>image {
  width: 227rpx;
  height: 196rpx;
  margin-top: 62rpx;
}

.null .top .kk {
  height: 28rpx;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 28rpx;
  margin-top: 10rpx;
}

.null .top .btn {
  width: 310rpx;
  height: 80rpx;
  background: rgba(142, 195, 31, 1);
  border-radius: 12rpx;
  margin: 0 auto;
  margin-top: 48rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.null .biao {
  height: 34rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Heavy;
  font-weight: bold;
  color: rgba(29, 32, 35, 1);
  line-height: 34rpx;
  padding: 0 20rpx;
  margin-top: 50rpx;
}
