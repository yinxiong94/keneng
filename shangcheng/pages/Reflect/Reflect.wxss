  margin-right: 10rpx;
}

.cont_text {
  font-size: 26rpx;
  color: #606972;
  display: flex;
  align-items: center;
  padding-top: 20rpx;
  padding-left: 20rpx;
}

.cont_but_text {
  width: 690rpx;
  height: 80rpx;
  background: rgba(142, 195, 31, 1);
  border-radius: 10px;
  color: #fff;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 40rpx;
}

.cont_buttext {
  display: flex;
  justify-content: center;
}
